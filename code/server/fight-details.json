{
"name":"fight-details",
	"fields":{
		"fighter_a_name":{"name":"fighter_a_name",
			"regex":"",
			"xPath":"//div[@class='fight_details_name'][1]/div[@class='name']/h2/a",
			"type":"STRING",
			"required":true,
			"notEmpty":true
		},
		"fighter_b_name":{"name":"fighter_b_name",
			"regex":"",
			"xPath":"//div[@class='fight_details_name'][2]/div[@class='name']/h2/a",
			"type":"STRING",
			"required":true,
			"notEmpty":true
		},
		"weight_class":{"name":"weight_class",
			"regex":"(?i)(?<capture>\\b(FLY|BANTAM|FEATHER|HEAVY|LIGHT|MIDDLE|WELTER|CATCH[\\s]|LIGHT[\\s]HEAVY)?WEIGHT|OPEN|SUPERFIGHT\\b)",
			"xPath":"//div[@class='header']",
			"type":"STRING",
			"required":true,
			"notEmpty":false
		},
		"finish_method":{"name":"finish_method",
			"regex":"",
			"xPath":"//table[@class='fight_details_info']/tbody/tr[2]/td[1]",
			"type":"STRING",
			"required":true,
			"notEmpty":true
		},
		"finish_details":{"name":"finish_details",
			"regex":"(?<capture>\\b[\\w\\W]+\\b)",
			"xPath":"//table[@class='fight_details_info']/tbody/tr[5]/td[1]/text()",
			"type":"STRING",
			"required":true,
			"notEmpty":false,
			"concat":true
		},
		"rounds_fought":{"name":"rounds_fought",
			"regex":"(?<capture>\\d+)",
			"xPath":"//table[@class='fight_details_info']/tbody/tr[2]/td[2]/text()",
			"type":"INTEGER",
			"required":true,
			"notEmpty":false
		},
		"time_in_last_round":{"name":"time_in_last_round",
			"regex":"(?<capture>\\d+:?\\d*)",
			"xPath":"//table[@class='fight_details_info']/tbody/tr[2]/td[3]/text()",
			"type":"TIME",
			"required":true,
			"notEmpty":false
		},
		"round_format":{"name":"round_format",
			"regex":"(?<capture>No Time Limit|[(]([^)]+[)]))",
			"xPath":"//table[@class='fight_details_info']/tbody/tr[2]/td[4]/text()",
			"type":"STRING",
			"required":true,
			"notEmpty":false
		},
		"referee_name":{"name":"referee_name",
			"regex":"",
			"xPath":"//table[@class='fight_details_info']/tbody/tr[2]/td[5]",
			"type":"STRING",
			"required":true,
			"notEmpty":false
		}
	}
}